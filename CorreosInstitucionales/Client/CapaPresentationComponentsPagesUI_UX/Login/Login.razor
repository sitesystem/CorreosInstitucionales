@page "/"

@attribute [AllowAnonymous]

<PageTitle>SACI | Inicio de Sesión</PageTitle>

@layout LoginLayout

<AuthorizeView>
    <Authorized>
        @{
            if (context.User.Identity?.IsAuthenticated == true)
                navigationManager.NavigateTo("/NuevaContraseña");
        }
    </Authorized>
    <NotAuthorized>
    </NotAuthorized>
</AuthorizeView>

<RadzenComponents />

<style type="text/css">
</style>

